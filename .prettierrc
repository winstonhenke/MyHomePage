{
  "trailingComma": "none",
  "printWidth": 120,
  "tabWidth": 2,
  "overrides": [
    {
      "files": "**/.prettierrc",
      "options": { "parser": "json" }
    },
    {
      "files": "**/*.code-workspace",
      "options": { "parser": "json" }
    },
    {
      "files": "**/*.md",
      "options": { "parser": "markdown" }
    }
  ]
}
